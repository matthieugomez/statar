fill_gap_ <- function(.data, ..., along_with, units = NULL, .dots, full = FALSE, roll = FALSE, rollends = if (roll=="nearest") c(TRUE,TRUE)#
             else if (roll>=0) c(FALSE,TRUE)#
             else c(TRUE,FALSE)) {#
  UseMethod("fill_gap_")#
}#
