    if (!identical(data:table::key(y),by$y)){#
      if (!setkey) y <- copy(y)#
      data.table::setkeyv(y, by$y)#
    }#
    out <- op#
    grouped_dt(out, groups(x))#
  })#
#
  f <- eval(template, parent.frame())#
  attr(f, "srcref") <- NULL # fix so prints correctly#
  f
)
