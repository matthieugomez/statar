demean <- function(x, ...){#
	dots <- lapply(list(...),function(x){if (is.atomic(x)) list(x) else x})#
	fe <- do.call(c, dots)#
	if (is.atomic(fe)){#
		fe <- list(as.factor(fe[!is.na(x)]))#
	} else{#
		fe <-  lapply(fe, function(z){as.factor(z[!is.na(x)])})#
	}#
	if (is.integer(x)){#
		x <- as.double(x)#
	}#
	x[!is.na(x)]<- demeanlist(x[!is.na(x)], fe)#
	x#
}
