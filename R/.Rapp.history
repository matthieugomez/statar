common_by <- function(by = NULL, x, y) {#
  if (!is.null(by)) {#
    return(list(#
      x = names(by) %||% by,#
      y = unname(by)#
    ))#
  }#
#
  by <- intersect(tbl_vars(x), tbl_vars(y))#
  if (length(by) == 0) {#
    stop("No common variables. Please specify `by` param.", call. = FALSE)#
  }#
  message("Joining by: ", capture.output(dput(by)))#
#
  list(#
    x = by,#
    y = by#
  )
}
